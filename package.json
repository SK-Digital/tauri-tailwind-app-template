{
  "name": "tauri-tailwind-template",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tailwind:build": "tailwindcss -i ./src/styles.css -o ./src/output.css",
    "tailwind:watch": "tailwindcss -i ./src/styles.css -o ./src/output.css --watch",
    "dev:tailwind": "concurrently \"npm run dev\" \"npm run tailwind:watch\"",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.4.14",
    "vite": "^4.4.4"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0"
  }
}
